integer getPatternValue(integer a, integer b, integer c) {
	if a == 1 && b == 1 && c == 1 {
		return 0;
	} else if a == 1 && b == 1 && c == 0 {
		return 1;
	} else if a == 1 && b == 0 && c == 1 {
		return 1;
	} else if a == 1 && b == 0 && c == 0 {
		return 1;
	} else if a == 0 && b == 1 && c == 1 {
		return 0;
	} else if a == 0 && b == 1 && c == 0 {
		return 1;
	} else if a == 0 && b == 0 && c == 1 {
		return 1;
	}
	return 0;
}